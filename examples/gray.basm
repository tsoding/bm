;; Generate N Gray Code numbers https://en.wikipedia.org/wiki/Gray_code

%include "./examples/natives.hasm"

    jmp main

%bind N 100

; i + 1

main:
    push 0     ; i
loop:
    dup 0
    dup 0
    push 1
    shr
    xor
    call dump_u64

    push 1
    plusi

    dup 0
    push N
    eqi
    not

    jmp_if loop
    
    halt

%entry main